{
	"name": "Public ports demo",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"remoteUser": "node",
	"forwardPorts": [
		3000,
		8080
	],
	"portsAttributes": {
		"3000": {
			"label": "Express API",
			"onAutoForward": "notify"
		}
	}
}